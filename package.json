    "git-commit-msg-linter": "^4.4.0",
    "husky": "^8.0.2",
    "ioredis-mock": "^8.8.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prisma": "^4.16.2",
